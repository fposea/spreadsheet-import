<mvc:View
    controllerName="basic.sapui5.app.controller.App"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    displayBlock="true"
>
    <App>
        <pages>
            <Page title="Bulk Upload">
                <content>
                    <VBox class="sapUiSmallMargin">
                        <Button
                            text="Package Upload"
                            press="onBulkUploadButtonPress"
                            class="sapUiSmallMarginEnd"
                        />
                        <Button
                            text="PackageMo Upload"
                            press="onBulkUploadButtonPress"
                            class="sapUiSmallMarginEnd"
                        />
                    </VBox>
                </content>
            </Page>
        </pages>
    </App>
</mvc:View>